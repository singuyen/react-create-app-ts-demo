{
  "name": "index-pages",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@types/react-router": "^4.0.17",
    "enzyme-adapter-react-16": "^1.1.0",
    "node-sass-chokidar": "0.0.3",
    "react": "^16.1.1",
    "react-dom": "^16.1.1",
    "react-height": "^3.0.0",
    "react-scripts-ts": "2.8.0"
  },
  "scripts": {
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch",
    "start-js": "react-scripts-ts start",
    "start": "npm-run-all watch-css -p start-js",
    "build-js": "react-scripts-ts build",
    "build": "npm-run-all build-css build-js",
    "test": "react-scripts-ts test --env=jsdom",
    "test-coverage": "CI=true react-scripts-ts test --env=jsdom --coverage --collectCoverageFrom=src/**/*.tsx",
    "eject": "react-scripts-ts eject"
  },
  "devDependencies": {
    "@racenet/racenet-ui": "^1.2.2",
    "@types/classnames": "^2.2.3",
    "@types/jest": "^21.1.6",
    "@types/lodash": "^4.14.85",
    "@types/node": "^8.0.53",
    "@types/react": "^16.0.23",
    "@types/react-dom": "^16.0.3",
    "@types/react-redux": "4.4.45",
    "@types/react-router-dom": "^4.2.1",
    "@types/react-router-redux": "^5.0.10",
    "@types/react-test-renderer": "^16.0.0",
    "@types/redux-thunk": "^2.1.0",
    "bootstrap": "^3.3.7",
    "classnames": "^2.2.5",
    "domain-task": "^3.0.3",
    "enzyme": "^3.2.0",
    "es6-object-assign": "^1.1.0",
    "moment": "^2.19.2",
    "node-sass-chokidar": "0.0.3",
    "npm-run-all": "^4.1.2",
    "react-redux": "^5.0.6",
    "react-resize-detector": "^3.0.1",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.8",
    "react-scripts-ts": "^2.8.0",
    "redux": "^3.7.2"
  }
}
